{
    "name": "robusta/magerun2",
    "description": "Tools for managing Magento projects and installations",
    "version": "v5.2.2",
    "license": "MIT",
    "homepage": "https://github.com/marwanatef2/n98-magerun2",
    "keywords": [
        "magento",
        "installer",
        "cli",
        "magerun2"
    ],
    "config": {
        "use-include-path": false,
        "preferred-install": "dist",
        "platform": {
            "php": "7.3.0"
        },
        "sort-packages": true
    },
    "require": {
        "php": "^7.3||^8.0",
        "ext-PDO": "*",
        "ext-SimpleXML": "*",
        "ext-dom": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-posix": "*",
        "ext-zip": "*",
        "dflydev/dot-access-data": "^3.0",
        "fakerphp/faker": "~1.16",
        "n98/junit-xml": "~1.0",
        "pbergman/tree-helper": "^1.0",
        "psy/psysh": "~0.7",
        "rmccue/requests": "^2.0",
        "symfony/console": "~4.4",
        "symfony/event-dispatcher": "~4.4",
        "symfony/finder": "~5.3",
        "symfony/process": "~4.4",
        "symfony/validator": "~4.4",
        "symfony/yaml": "~5.3",
        "twig/twig": "~3.3"
    },
    "require-dev": {
        "bamarni/symfony-console-autocomplete": "^1.2.0",
        "captainhook/captainhook": "^5.3",
        "friendsofphp/php-cs-fixer": "~3.4.0",
        "mikey179/vfsstream": "^1.6",
        "phpunit/phpunit": "^9",
        "roave/security-advisories": "dev-master",
        "seld/phar-utils": "~1.2.0"
    },
    "suggest": {
         "n98/magerun2-dist": "Deploys only the phar file. Use this to prevent dependency conflicts."
    },
    "autoload": {
        "psr-4": {
            "Robusta\\": "src/Robusta"
        }
    },
    "authors": [
        {
            "name": "Marwan Atef",
            "email": "marwan.atef@robustastudio.com"
        }
    ],
    "bin": [
        "bin/robusta-magerun2"
    ],
    "prefer-stable": true
}
